{
  "name": "ink-engine",
  "version": "0.1.0",
  "description": "Process documents as W3C Publications",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "lint": "healthier && prettier --write --plugin-search-dir=. '**/*.{js,json,css,yml,svelte}'",
    "pretest": "npm run lint",
    "prepublishOnly": "npm run lint",
    "test": "tap tests/*.test.js",
    "snapshots": "TAP_SNAPSHOT=1 npm run test && prettier --write --plugin-search-dir=. '**/*.{js,json,css,yml,svelte}'"
  },
  "engines": {
    "node": "10"
  },
  "healthier": {
    "ignore": [
      "__sapper__",
      "rollup.config.js"
    ]
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "dompurify": "^2.0.7",
    "hast-util-from-dom": "^2.0.1",
    "hast-util-has-property": "^1.0.3",
    "hast-util-is-element": "^1.0.3",
    "hast-util-select": "^3.0.1",
    "hast-util-to-text": "^1.0.1",
    "hastscript": "^5.1.1",
    "jsdom": "^15.2.1",
    "jsonwebtoken": "^8.5.1",
    "mammoth": "^1.4.9",
    "mime": "^2.4.4",
    "node-fetch": "^2.6.0",
    "postcss": "^7.0.23",
    "postcss-prefix-selector": "^1.7.2",
    "postcss-selector-parser": "^6.0.2",
    "postcss-value-parser": "^4.0.2",
    "property-information": "^5.3.0",
    "rehype-slug": "^2.0.3",
    "rehype-stringify": "^6.0.1",
    "retext-latin": "^2.0.4",
    "rimraf": "^3.0.0",
    "sharp": "^0.23.3",
    "to-vfile": "^6.0.0",
    "unified": "^8.4.2",
    "unified-engine": "^7.0.0",
    "unist-util-size": "^1.1.1",
    "unist-util-visit": "^2.0.1",
    "unzipper": "^0.10.5",
    "valid-data-url": "^2.0.0",
    "vfile": "^4.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "healthier": "3.2.0",
    "prettier": "1.19.1",
    "tap": "14.9.2",
    "typescript": "^3.7.2"
  }
}
